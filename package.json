{
  "name": "fasock",
  "version": "1.0.0",
  "description": "Explore socket.io library with fastify",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "build": "tsc",
    "watch": "nodemon src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gani-laboratory/fasock.git"
  },
  "keywords": [
    "http",
    "fastify",
    "rest api",
    "socket.io",
    "nodejs",
    "nodejs",
    "webtransport",
    "websocket"
  ],
  "author": "Ahmad Gani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gani-laboratory/fasock/issues"
  },
  "homepage": "https://github.com/Gani-laboratory/fasock#readme",
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -r tsconfig-paths/register"
    },
    "verbose": true,
    "watch": "src"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.5.0",
    "pg": "^8.8.0",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
