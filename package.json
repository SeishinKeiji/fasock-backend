{
  "name": "fasock",
  "version": "1.0.0",
  "description": "Explore socket.io library with fastify",
  "main": "index.js",
  "scripts": {
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d src/database/datasource.ts src/database/seeder.ts",
    "schema:drop": "typeorm-ts-node-esm schema:drop -d ./src/database/datasource.ts",
    "start": "node dist",
    "build": "tsc",
    "watch": "nodemon src/index.ts",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gani-laboratory/fasock.git"
  },
  "keywords": [
    "http",
    "fastify",
    "rest api",
    "socket.io",
    "nodejs",
    "nodejs",
    "webtransport",
    "websocket"
  ],
  "author": "Ahmad Gani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gani-laboratory/fasock/issues"
  },
  "homepage": "https://github.com/Gani-laboratory/fasock#readme",
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -r tsconfig-paths/register"
    },
    "verbose": true,
    "watch": "src"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.5.0",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
